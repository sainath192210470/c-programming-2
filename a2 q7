#include <stdio.h>

int main() {
    int n, sum = 0;
    printf("Enter a positive integer n: ");
    scanf("%d", &n);

    for (int i = 1; i <= n; i++) {
        if (i % 2 != 0) {  // Check if i is odd
            sum += i;      // Add i to the sum if it's odd
        }
    }

    printf("Sum of all odd numbers from 1 to %d is %d\n", n, sum);
    return 0;
}
