#include <stdio.h>

int main() {
    char c;
    printf("Enter a character: ");
    scanf("%c", &c);

    // Using conditional operator to check if character is an alphabet
    (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z') ? printf("%c is an alphabet.", c) : printf("%c is not an alphabet.", c);

    return 0;
}
